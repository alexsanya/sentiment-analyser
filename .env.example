# Twitter API Configuration
TWITTERAPI_KEY=your_twitter_api_key_here

# Application Environment (development/production)
ENVIRONMENT=development

# RabbitMQ Configuration
# SECURITY: Use strong credentials in production and never commit real credentials to version control
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_QUEUE=tweet_events
RABBITMQ_USERNAME=your_rabbitmq_username
RABBITMQ_PASSWORD=your_strong_rabbitmq_password
